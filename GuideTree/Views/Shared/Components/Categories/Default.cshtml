@model List<Category>

  <div>
        @foreach(var category in Model)
        {
            <a asp-controller="Home" asp-action="Index" asp-route-category="@category.Url" class="@(ViewBag.SelectedCategory == category.Url.ToString()?"active":"") text-dark text-decoration-none">
                    @category.Name
            </a>   
        }
  </div>